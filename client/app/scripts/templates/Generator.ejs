
<div class="container">
    

    <h1> <%= name %> <small> <a> <%=packageName%></a>.<a><%=moduleName%></a>.<%=name%></small> </h1>
    <h3> <span class='lead'> version </span> <%=version%> <span class='lead'> by </span> <%=author%> </h3>
    <p class="lead" ><%=description%>.</p>

    <% for (prop in templates) { %>
      <h3> <%=prop%> </h3>
      <pre><%=templates[prop]%></pre> 
    <% } %>

</div>

    